{
  "modelJson" : {
    "baseURL" : "http://example.org/",
    "languageCode" : "en-us",
    "title" : "My New Hugo Site",
    "paginate" : 10
  },
  "jsonFields" : [
    {
      "key" : "baseURL",
      "type": "input",
      "templateOptions": {
        "label": "baseURL",
        "placeholder": "https://example.org"
      }
    },
    {
      "key" : "languageCode",
      "type": "input",
      "templateOptions": {
        "label": "languageCode",
        "placeholder": "en-us"
      }
    },
    {
      "key" : "languageName",
      "type": "input",
      "templateOptions": {
        "label": "languageName",
        "placeholder": "English (US)"
      }
    },
    {
      "key" : "title",
      "type": "input",
      "templateOptions": {
        "label": "title",
        "placeholder": "My Awesome Hugo Site"
      }
    },
    {
      "key" : "theme",
      "type": "input",
      "templateOptions": {
        "label": "theme",
        "placeholder": "Theme to use (located by default in /themes/THEMENAME/)."
      }
    },
    {
      "key" : "themesDir",
      "type": "input",
      "templateOptions": {
        "label": "themesDir",
        "placeholder": "The directory where Hugo reads the themes from."
      }
