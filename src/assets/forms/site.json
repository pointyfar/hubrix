    },
    {
      "key" : "pygmentsCodeFencesGuessSyntax",
      "type": "checkbox",
      "templateOptions": {
        "label": "pygmentsCodeFencesGuessSyntax"
      }
    },
    {
      "key" : "pygmentsStyle",
      "type": "input",
      "templateOptions": {
