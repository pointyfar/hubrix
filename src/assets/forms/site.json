    {
      "key" : "hasCJKLanguage",
      "type": "checkbox",
      "templateOptions": {
        "label": "hasCJKLanguage"
      }
    },
    {
      "key" : "layoutDir",
      "type": "input",
      "templateOptions": {
        "label": "layoutDir",
        "placeholder": "The directory from where Hugo reads layouts (templates)."
      }
