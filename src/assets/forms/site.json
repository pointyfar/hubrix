      }
    },
    {
      "key": "disableKinds",
      "type": "select",
      "templateOptions": {
        "label": "disableKinds",
        "multiple": true,
        "options": [
          {"label": "Page", "value": "page"},
          {"label": "Home", "value": "home"},
          {"label": "Section", "value": "section"},
          {"label": "Taxonomy", "value": "taxonomy"},
          {"label": "Taxonomy Term", "value": "taxonomyTerm"},
          {"label": "RSS", "value": "rss"},
          {"label": "Sitemap", "value": "sitemap"},
          {"label": "robotsTXT", "value": "robotsTXT"},
          {"label": "404", "value": "404"}
        ]
      }
    },
    {
      "key": "disableKinds2",
      "type": "array",
      "fieldArray": {
        "type": "input",
        "templateOptions": {
          "label": "Add Kind",
          "placeholder": "'page', 'home', 'section', 'taxonomy', 'taxonomyTerm', 'RSS', 'sitemap', 'robotsTXT', '404'"
