    {
      "key" : "pygmentsUseClasses",
      "type": "checkbox",
      "templateOptions": {
        "label": "pygmentsUseClasses"
      }
    },
    {
      "key" : "relativeURLs",
      "type": "checkbox",
      "templateOptions": {
        "label": "relativeURLs"
      }
    },
    {
