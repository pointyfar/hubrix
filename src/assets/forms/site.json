      "type": "input",
      "templateOptions": {
        "label": "sectionPagesMenu",
        "placeholder": "main"
      }
    },
    {
      "key" : "googleAnalytics",
      "type": "input",
      "templateOptions": {
        "label": "googleAnalytics",
        "placeholder": "Google Analytics tracking ID."
      }
    },
    {
      "key" : "archetypeDir",
      "type": "input",
      "templateOptions": {
        "label": "archetypeDir",
        "placeholder": "The directory where Hugo finds archetype files (content templates)."
      }
    },
    {
      "key" : "buildDrafts",
      "type": "checkbox",
      "templateOptions": {
        "label": "buildDrafts"
      }
    },
    {
      "key" : "buildExpired",
      "type": "checkbox",
      "templateOptions": {
        "label": "buildExpired"
      }
    },
    {
      "key" : "buildFuture",
      "type": "checkbox",
      "templateOptions": {
        "label": "buildFuture"
      }
    },
    {
      "key" : "canonifyURLS",
      "type": "checkbox",
      "templateOptions": {
        "label": "canonifyURLS"
      }
    },
    {
      "key" : "contentDir",
      "type": "input",
      "templateOptions": {
        "label": "contentDir",
        "placeholder": "The directory from where Hugo reads content files."
      }
    },
    {
      "key" : "dataDir",
      "type": "input",
      "templateOptions": {
        "label": "dataDir",
        "placeholder": "The directory from where Hugo reads data files."
      }
