      "type": "checkbox",
      "templateOptions": {
        "label": "disablePathToLower"
      }
    },
    {
      "key" : "enableEmoji",
      "type": "checkbox",
      "templateOptions": {
        "label": "enableEmoji"
      }
    },
    {
      "key" : "enableGitInfo",
      "type": "checkbox",
      "templateOptions": {
        "label": "enableGitInfo"
      }
    },
    {
      "key" : "enableMissingTranslationPlaceholders",
      "type": "checkbox",
      "templateOptions": {
        "label": "enableMissingTranslationPlaceholders"
      }
    },
    {
      "key" : "enableRobotsTXT",
      "type": "checkbox",
      "templateOptions": {
        "label": "enableRobotsTXT"
      }
    },
    {
      "key" : "footnoteAnchorPrefix",
      "type": "input",
      "templateOptions": {
        "label": "footnoteAnchorPrefix",
        "placeholder": "Prefix for footnote anchors."
