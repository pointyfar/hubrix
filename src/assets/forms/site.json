      "key": "metaDataFormat",
      "type": "select",
      "templateOptions": {
        "label": "Metadata Format",
        "multiple": true,
        "options": [
          {"label": "TOML", "value": "toml"},
          {"label": "YAML", "value": "yaml"},
          {"label": "JSON", "value": "json"}
        ]
      }
    },
    {
      "key" : "newContentEditor",
      "type": "input",
      "templateOptions": {
        "label": "newContentEditor",
        "placeholder": "The editor to use when creating new content."
      }
    },
    {
      "key" : "noChmod",
      "type": "checkbox",
      "templateOptions": {
        "label": "noChmod"
      }
    },
    {
      "key" : "noTimes",
      "type": "checkbox",
      "templateOptions": {
        "label": "noTimes"
      }
    },
    {
      "key" : "paginatePath",
      "type": "input",
      "templateOptions": {
