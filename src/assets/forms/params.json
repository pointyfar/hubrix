{
  "modelJson" : {
    "baseURL" : "http://example.org/",
    "languageCode" : "en-us",
    "title" : "My New Hugo Site",
    "paginate" : 10
  },
  "jsonFields" : [
    {
      "key" : "baseURL",
      "type": "input",
      "templateOptions": {
        "label": "baseURL",
        "placeholder": "https://example.org",
        "description": "Hostname (and path) to the root, e.g. http://bep.is/"
      }
    },
    {
      "key" : "languageCode",
      "type": "input",
      "templateOptions": {
        "label": "languageCode",
        "placeholder": "en-us",
        "description": "The siteâ€™s language code."
      }
    },
    {
      "key" : "title",
      "type": "input",
      "templateOptions": {
        "label": "title",
        "placeholder": "My Awesome Hugo Site",
        "description": "Site title."
      }
    },
    {
      "key" : "paginate",
      "type": "input",
      "templateOptions": {
        "label": "paginate",
        "type": "number"
      }
    },
    {
      "key": "disableKinds",
      "type": "select",
      "templateOptions": {
        "label": "disableKinds",
        "multiple": true,
        "options": [
          {"label": "Page", "value": "page"},
          {"label": "Home", "value": "home"},
          {"label": "Section", "value": "section"},
          {"label": "Taxonomy", "value": "taxonomy"},
          {"label": "Taxonomy Term", "value": "taxonomyTerm"},
          {"label": "RSS", "value": "rss"},
          {"label": "Sitemap", "value": "sitemap"},
          {"label": "robotsTXT", "value": "robotsTXT"},
          {"label": "404", "value": "404"}
        ],
        "description": "Enable disabling of all pages of the specified Kinds. Allowed values in this list: 'page', 'home', 'section', 'taxonomy', 'taxonomyTerm', 'RSS', 'sitemap', 'robotsTXT', '404'."
      }
    },
    {
      "key" : "imaging",
      "wrappers": ["panel"],
      "templateOptions": {
        "label": "imaging",
        "description": "You can configure an imaging section in config.toml with default image processing options:"
      },
      "fieldGroup": [
        {
          "key" : "resampleFilter",
          "type": "input",
          "templateOptions": {
            "label": "resampleFilter",
            "placeholder": "box",
            "description": "Default resample filter used for resizing."
          }
        },
        {
          "key" : "quality",
          "type": "input",
          "templateOptions": {
            "label": "quality",
            "type": "number",
            "placeholder": 75,
            "description": "Default JPEG quality setting."
          }
        },
        {
          "key": "anchor",
          "type": "select",
          "templateOptions": {
            "label": "anchor",
            "multiple": false,
            "options": [
              {"label": "Smart", "value": "Smart"},
              {"label": "Center", "value": "Center"},
              {"label": "TopLeft", "value": "TopLeft"},
              {"label": "Top", "value": "Top"},
              {"label": "TopRight", "value": "TopRight"},
              {"label": "Left", "value": "Left"},
              {"label": "Right", "value": "Right"},
              {"label": "BottomLeft", "value": "BottomLeft"},
              {"label": "Bottom", "value": "Bottom"},
              {"label": "BottomRight", "value": "BottomRight"}
            ],
            "description": "Anchor used when cropping pictures. "
          }
        }
      ]
    }
  ]
}