{
  "name": "hubrix",
  "version": "0.1.5",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --output-hashing=none",
    "package": "concat dist/hubrix/{runtime,polyfills,scripts,main}.js | gzip > hubrix.js.gz",
    "build-serve": "http-server --gzip",
    "build-pack-serve": "yarn run build && yarn run package && yarn run build-serve",
    "test": "ng test",
    "lint": "ng lint",
    "stats": "webpack-bundle-analyzer tmp/dist/stats.json",
    "e2e": "ng e2e"
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  },
  "private": true,
  "dependencies": {
